                <xpath>/Defs/XenotypeDef[defName="MBSXT_Damphir"]/genes</xpath>
                <value>
                    <li>Libido_VeryLow</li>
                </value>
            </li>

            <li Class="PatchOperationAdd" MayRequire="LibidoGenes.Mod">
                <xpath>/Defs/XenotypeDef[defName="MBSXT_Viperal"]/genes</xpath>
                <value>
                    <li>Libido_ExtremelyLow</li>
                </value>
            </li>

            <li Class="PatchOperationAdd" MayRequire="LibidoGenes.Mod">
                <xpath>/Defs/XenotypeDef[defName="MBSXT_Artifexian"]/genes</xpath>
                <value>
                    <li>Libido_AboveAverage</li>
                </value>
            </li>

            <li Class="PatchOperationAdd" MayRequire="LibidoGenes.Mod">
                <xpath>/Defs/XenotypeDef[defName="MBSXT_Harmonite"]/genes</xpath>
                <value>
                    <li>Libido_ExtremelyLow</li>
                </value>
            </li>

            <li Class="PatchOperationAdd" MayRequire="LibidoGenes.Mod">
                <xpath>/Defs/XenotypeDef[defName="MBSXT_Oni"]/genes</xpath>
                <value>
                    <li>Libido_VeryHigh</li>
                </value>
            </li>
        </operations>
