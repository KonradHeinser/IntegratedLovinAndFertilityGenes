<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationSequence">
        <operations>
            <li Class="PatchOperationAdd" MayRequire="LibidoGenes.Mod">
                <xpath>/Defs/XenotypeDef[defName="AG_Fleetkind"]/genes</xpath>
                <value>
                    <li>Libido_ExtremelyLow</li>
                </value>
            </li>

            <li Class="PatchOperationAdd" MayRequire="LibidoGenes.Mod">
                <xpath>/Defs/XenotypeDef[defName="AG_Lapis"]/genes</xpath>
                <value>
                    <li>Libido_VeryLow</li>
                </value>
            </li>

            <li Class="PatchOperationAdd" MayRequire="LibidoGenes.Mod">
                <xpath>/Defs/XenotypeDef[defName="AG_Wretch"]/genes</xpath>
                <value>
                    <li>Libido_ExtremelyLow</li>
                </value>
            </li>

            <li Class="PatchOperationAdd" MayRequire="LibidoGenes.Mod">
                <xpath>/Defs/XenotypeDef[defName="AG_Taukai"]/genes</xpath>
                <value>
                    <li>Libido_VeryLow</li>
                </value>
            </li>

            <li Class="PatchOperationAdd" MayRequire="LibidoGenes.Mod">
                <xpath>/Defs/XenotypeDef[defName="AG_Hiveling"]/genes</xpath>
                <value>
                    <li>Libido_ExtremelyLow</li>
                </value>
            </li>

            <li Class="PatchOperationAdd" MayRequire="LibidoGenes.Mod">
                <xpath>/Defs/XenotypeDef[defName="AG_MindDevourer"]/genes</xpath>
                <value>
                    <li>Libido_ExtremelyLow</li>
                </value>
            </li>

            <li Class="PatchOperationAdd" MayRequire="LibidoGenes.Mod">
                <xpath>/Defs/XenotypeDef[defName="AG_Efreet"]/genes</xpath>
                <value>
                    <li>Libido_VeryHigh</li>
                </value>
            </li>


            <li Class="PatchOperationFindMod" MayRequire="LibidoGenes.Mod">
		        <mods>
			        <li>Vanilla Races Expanded - Sanguophage</li>
		        </mods>
                <match Class="PatchOperationSequence">
		            <operations>
                        <li Class="PatchOperationAdd" MayRequire="LibidoGenes.Mod">
                            <xpath>/Defs/XenotypeDef[defName="AG_Malachai"]/genes</xpath>
                            <value>
                                <li>Libido_ExtremelyLow</li>
                            </value>
                        </li>
                    </operations>
                </match>
            </li>

            <li Class="PatchOperationFindMod" MayRequire="LibidoGenes.Mod">
		        <mods>
			        <li>Vanilla Races Expanded - Phytokin</li>
		        </mods>
                <match Class="PatchOperationSequence">
		            <operations>
                        <li Class="PatchOperationAdd" MayRequire="LibidoGenes.Mod">
                            <xpath>/Defs/XenotypeDef[defName="VRE_Ocularkin"]/genes</xpath>
                            <value>
                                <li>Libido_VeryLow</li>
                            </value>
                        </li>
                    </operations>
                </match>
            </li>
        </operations>
    </Operation>
</Patch>
